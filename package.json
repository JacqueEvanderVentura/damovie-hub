{
	"name": "damovie-hub",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint-fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
		"prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .prettierignore",
		"prettier-fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .prettierignore",
		"format": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix && prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"preview": "vite preview"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.7",
		"classnames": "^2.3.2",
		"dotenv": "^16.3.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.47.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.16.0",
		"react-tabs": "^6.0.2",
		"redux": "^4.2.1",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.8.6",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.1.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^9.0.0",
		"eslint-config-standard-with-typescript": "^39.1.1",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"sass": "^1.69.3",
		"tailwindcss": "^3.3.3",
		"typescript": "*",
		"vite": "^4.4.5"
	}
}
